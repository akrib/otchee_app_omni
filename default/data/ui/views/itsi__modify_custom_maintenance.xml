<form script="common\assets\js\step_wizard_control\step_wizard_control_v2.js,common\assets\js\modal_popup\modal_popup.js,omni/omni.js,common/assets/js/help_tour/help_tour.js" hideEdit="true" version="1.1" theme="light">
  <label>ITSI : Modify Custom Maintenance</label>
  <init>
    <eval token="update">now()</eval>
    <set token="stepLabel">firstStep</set>
    <set token="modal_header"></set>
    <set token="modal_content"></set>
    <set token="modal_footer"></set>
    <set token="hide">1</set>
    <set token="field_sup">0</set>
  </init>
  <search>
    <query>
      |rest /services/authentication/users splunk_server=local
      | search
      [| rest /services/authentication/current-context splunk_server=local
      | rename username as title
      | fields title]
      | table title realname email
    </query>
    <done>
      <set token="login">$result.title$</set>
      <set token="realname">$result.realname$</set>
      <set token="email">$result.email$</set>
      <set token="form.email">$result.email$</set>
    </done>
  </search>
  <fieldset submitButton="false"></fieldset>
  
  <row>
    <panel>
      <html>
        <h2 style="font-size:16px !important;">
          <a href="./accueil">
            <img src="/static/app/otchee_app_omni/media/left-chevron-blue.png" width="32px"/>
            <strong>Revenir à l'écran "Menu des Maintenances"</strong>
          </a>
        </h2>
      </html>
    </panel>
  </row>
  
  <row depends="$DT_ID$" rejects="$update_full_loading$">
    <panel>
      <html>
        <center>
          <h1>Mise à jour de maintenance custom</h1>
        </center>
      </html>
    </panel>
  </row>
  
  <row depends="$DT_ID$" rejects="$update_full_loading$">
    <panel>
      <html>
        <div class="Loadercontainer" height="100%">
          <div class="rel">
            <div class="center"/>
            <div class="ring ring_1">
              <div class="part part_1"/>
            </div>
            <div class="ring ring_2">
              <div class="part part_2"/>
              <div class="part part_3"/>
            </div>
          </div>
        </div>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <br/>
        <center>
          <H2>Chargement des données en cours</H2>
        </center>
      </html>
    </panel>
  </row>
  
  <row>
    <panel>
      <html>
        <section class="navbar custom-navbar" role="navigation">
          <div class="container">
            <div class="navbar-header">
              <h1 class="navbar-brand" style="font-size: 30px !important;">
                <span class="omni-title-first-part">
                  <img src="/static/app/otchee_app_omni/media/logo_omni.png" alt="Omni Logo" style="width: 200px; height: auto;"/>
                </span>
                <span class="omni-title-second-part"> Modify Custom Maintenance</span>
              </h1>
            </div>
          </div>
        </section>
      </html>
    </panel>
    <panel depends="$DT_ID$,$update_full_loading$">
      <html>
        <div id="step_control_wizard_holder"/>
        <p id="swc_step_0" data-label="Champs Custom" data-value="" data-showPreviousButton="0" data-validateToken="dt_filter_selected"/> 
        <p id="swc_step_1" data-label="Pattern" data-validateToken="dt_pattern_selected"/>   
        <p id="swc_step_2" data-label="Periode" data-validateToken="periode_selected" data-showNextButton="false"/>     
        <div id="step-control-wizard"/>
      </html>
    </panel>
  </row>
  
  <row depends="$step_0$,$DT_ID$,$update_full_loading$">
    <panel>
      <html>
        <h1>Champ Custom</h1>
      </html>
    </panel>
  </row>
  
  <row depends="$step_0$,$DT_ID$,$update_full_loading$">
    <panel id="custom_left_01">
      <input type="checkbox" token="show_custom_field_sup" id="show_sup">
        <label></label>
        <choice value="1">Ajouter un deuxième champ custom</choice>
        <delimiter> </delimiter>
        <change>
          <condition value="1">
            <set token="field_sup">1</set>
          </condition>
          <condition>
            <set token="field_sup">0</set>
          </condition>
        </change>
      </input>
    </panel>
    <panel id="custom_right_01">
      <input type="checkbox" token="not_1" id="field_not_1">
        <label></label>
        <choice value="NOT">NOT</choice>
        <delimiter> </delimiter>
        <default></default>
      </input>
      <input type="text" token="field_name_1" id="field_name_1">
        <label>Nom du champ Custom 1</label>
        <default></default>
      </input>
      <input type="dropdown" token="field_type_1" id="field_select_1_1">
        <label>Type de champ</label>
        <choice value="string">string</choice>
        <choice value="number">number</choice>
        <default>string</default>
        <initialValue>string</initialValue>
        <change>
          <set token="field_operator_1">=</set>
          <set token="form.field_operator_1">=</set>
        </change>
      </input>
      <input type="dropdown" token="field_operator_1" id="field_select_1_2">
        <label>Operateur</label>
        <default>=</default>
        <initialValue>=</initialValue>
        <fieldForLabel>label</fieldForLabel>
        <fieldForValue>value</fieldForValue>
        <search>
          <query>
| makeresults 
| eval filter=split(case("$field_type_1$"="string","Egal à#=#0,Différent de#!=#1,Est vide#inull()#2,N'est pas vide#isnotnul()#3,Contient#LIKE#4,Commence par#LIKEC#5,Fini par#LIKEF#6","$field_type_1$"="number","Egal à#=#0,Différent de#!=#1,Plus petit ou égal à#&lt;=#2,Plus grand ou égal à#&gt;=#3,Plus petit que#&lt;#4,Plus grand que#&gt;#5,Est vide#inull()#6,N'est pas vide#isnotnul()#7",true(),"="),",")
| stats count as _c by filter
| eval filter=split(filter,"#"), label=mvindex(filter,0), value=mvindex(filter,1), order=mvindex(filter,2)
| sort order
          </query>
        </search>
      </input>
      <input type="text" token="field_value_1" id="field_value_1">
        <label>Valeur du champ 1</label>
        <default></default>
      </input>
    </panel>
  </row>
  
  <row depends="$step_0$,$show_custom_field_sup$,$DT_ID$,$update_full_loading$">
    <panel id="custom_left_02" depends="$show_custom_field_sup$">
      <input type="dropdown" token="operator" id="operator">
        <label>Choisissez l'operateur</label>
        <choice value="AND">AND</choice>
        <choice value="OR">OR</choice>
        <default>AND</default>
        <initialValue>AND</initialValue>
      </input>
    </panel>
    <panel id="custom_right_02" depends="$show_custom_field_sup$">
      <input type="checkbox" token="not_2" id="field_not_2">
        <label></label>
        <choice value="NOT">NOT</choice>
        <delimiter> </delimiter>
        <default></default>
      </input>
      <input type="text" token="field_name_2">
        <label>Nom du champ Custom 2</label>
        <default></default>
      </input>
      <input type="dropdown" token="field_type_2" id="field_select_2_1">
        <label>Type de champ</label>
        <choice value="other_field">un autre champ</choice>
        <choice value="string">string</choice>
        <choice value="number">number</choice>
        <default>string</default>
        <initialValue>string</initialValue>
        <change>
          <set token="field_operator_2">=</set>
          <set token="form.field_operator_2">=</set>
        </change>
      </input>
      <input type="dropdown" token="field_operator_2" id="field_select_2_2">
        <label>Operateur</label>
        <default>=</default>
        <initialValue>=</initialValue>
        <fieldForLabel>label</fieldForLabel>
        <fieldForValue>value</fieldForValue>
        <search>
          <query>
| makeresults 
| eval filter=split(case("$field_type_2$"="string","Egal à#=#0,Différent de#!=#1,Est vide#inull()#2,N'est pas vide#isnotnul()#3,Contient#LIKE#4,Commence par#LIKEC#5,Fini par#LIKEF#6","$field_type_2$"="number","Egal à#=#0,Différent de#!=#1,Plus petit ou égal à#&lt;=#2,Plus grand ou égal à#&gt;=#3,Plus petit que#&lt;#4,Plus grand que#&gt;#5,Est vide#inull()#6,N'est pas vide#isnotnul()#7",true(),"="),",")
| stats count as _c by filter
| eval filter=split(filter,"#"), label=mvindex(filter,0), value=mvindex(filter,1), order=mvindex(filter,2)
| sort order
          </query>
        </search>
      </input>
      <input type="text" token="field_value_2">
        <label>Valeur du champ 2</label>
        <default></default>
      </input>
    </panel>
  </row>
  
  <row depends="$step_0$,$DT_ID$,$update_full_loading$">
    <panel>
      <table id="show_results">
        <search>
          <query>
|makeresults 
|eval 
 not_1=coalesce("$not_1$",""), 
 field_name_1=coalesce("$field_name_1$",""), 
 field_type_1=coalesce("$field_type_1$",""),
 value_quote_1=case(field_type_1="string","\"",true(),""),
 field_begin_perc_1=if("$field_operator_1$"="LIKE" OR "$field_operator_1$"="LIKEC","%",""), 
 field_end_perc_1=if("$field_operator_1$"="LIKE" OR "$field_operator_1$"="LIKEF","%",""),
 field_operator_1=case("$field_operator_1$"="isnull()","isnull(", "$field_operator_1$"="isnotnull()","isnotnull(","$field_operator_1$"="LIKE" OR "$field_operator_1$"="LIKEC" OR "$field_operator_1$"="LIKEF"," LIKE ",true(),"$field_operator_1$"),
 field_value_1=coalesce("$field_value_1$",""),
 field_close_parenthesis_1=if("$field_operator_1$"="isnull()" OR "$field_operator_1$"="isnotnull()",")",""),
 operator=coalesce("$operator$",""),
 not_2=coalesce("$not_2$",""),
 field_name_2=coalesce("$field_name_2$",""), 
 field_type_2=coalesce("$field_type_2$",""),
 value_quote_2=case(field_type_2="string","\"",true(),""),
 field_begin_perc_2=if("$field_operator_2$"="LIKE" OR "$field_operator_2$"="LIKEC","%",""), 
 field_end_perc_2=if("$field_operator_2$"="LIKE" OR "$field_operator_2$"="LIKEF","%",""),
 field_operator_2=case("$field_operator_2$"="isnull()","isnull(", "$field_operator_2$"="isnotnull()","isnotnull(","$field_operator_2$"="LIKE" OR "$field_operator_2$"="LIKEC" OR "$field_operator_2$"="LIKEF"," LIKE ",true(),"$field_operator_2$"),
 field_value_2=coalesce("$field_value_2$",""),
 field_close_parenthesis_2=if("$field_operator_2$"="isnull()" OR "$field_operator_2$"="isnotnull()",")",""),
 export=not_1+if(len(not_1)=0,""," ")+field_name_1+field_operator_1+value_quote_1+field_begin_perc_1+field_value_1+field_end_perc_1+value_quote_1+field_close_parenthesis_1+if(coalesce("$field_sup$","")="1"," "+operator+" "+not_2+if(len(not_2)=0,""," ")+field_name_2+field_operator_2+value_quote_2+field_begin_perc_2+field_value_2+field_end_perc_2+value_quote_2+field_close_parenthesis_2,"")
 | eval is_valid=if(match(export, "^(?:NOT\\s+)?[a-zA-Z_][a-zA-Z0-9_]*(?:\\s*(?:=|!=|&lt;=|&gt;=|&lt;|&gt;|LIKE)\\s*(?:\"[^\"]*\"|[a-zA-Z_][a-zA-Z0-9_]*|\\d+(?:\\.\\d+)?)|isnull\\(\\)|isnotnull\\(\\))(?:\\s+(?:AND|OR)\\s+(?:NOT\\s+)?[a-zA-Z_][a-zA-Z0-9_]*(?:\\s*(?:=|!=|&lt;=|&gt;=|&lt;|&gt;|LIKE)\\s*(?:\"[^\"]*\"|[a-zA-Z_][a-zA-Z0-9_]*|\\d+(?:\\.\\d+)?)|isnull\\(\\)|isnotnull\\(\\)))?$") OR len(export)=0, "Valide", "Invalide")
|table export, is_valid
          </query>
          <earliest>$earliest$</earliest>
          <latest>$latest$</latest>
          <done>
            <condition match='$result.is_valid$="Valide"'>
              <set token="dt_filter_selected">$result.export$</set>
            </condition>
            <condition>
              <unset token="dt_filter_selected"></unset>
            </condition>
          </done>
        </search>
        <option name="count">1</option>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
  
  <row depends="$step_1$">
    <panel>
      <html>
        <h1>PATTERN</h1>
      </html>
    </panel>
  </row>
  
  <row depends="$step_1$">
    <panel>
      <input type="dropdown" token="pattern_type">
        <label>Pattern type</label>
        <choice value="exist">Pattern existant</choice>
        <choice value="new">Nouveau pattern</choice>
        <default>exist</default>
        <initialValue>exist</initialValue>
        <change>
          <condition match="$pattern_type$=&quot;exist&quot;">
            <set token="pattern_exist">1</set>
            <unset token="pattern_new"></unset>
            <unset token="dt_pattern_selected"></unset>
            <unset token="dt_patern_select"></unset>
            <unset token="dt_pattern_text"></unset>
          </condition>
          <condition match="$pattern_type$=&quot;new&quot;">
            <set token="pattern_new">1</set>
            <unset token="pattern_exist"></unset>
            <unset token="dt_pattern_selected"></unset>
            <unset token="dt_patern_select"></unset>
            <unset token="dt_pattern_text"></unset>
          </condition>
        </change>
      </input>
      <input type="dropdown" token="dt_patern_select" depends="$pattern_exist$">
        <label>Downtime Pattern</label>
        <change>
          <set token="dt_pattern_selected">$dt_patern_select$</set>
        </change>
        <fieldForLabel>dt_pattern</fieldForLabel>
        <fieldForValue>dt_pattern</fieldForValue>
        <search>
          <query>
| inputlookup omni_kv_def 
| stats count as _c by dt_pattern
| appendpipe
[| stats count 
 | where count=0
 | eval dt_pattern="-"
 | table dt_pattern]
          </query>
        </search>
      </input>
      <input type="text" token="dt_pattern_text" depends="$pattern_new$">
        <label>Downtime pattern</label>
        <default></default>
        <change>
          <set token="dt_pattern_selected">$dt_pattern_text$</set>
        </change>
      </input>
    </panel>
  </row>
  
  <row depends="$step_2$">
    <panel>
      <html>
        <center>
          <div id="downtime"/>
        </center>
      </html>
    </panel>
  </row>
  
  <row depends="$step_2$">
    <panel>
      <input type="checkbox" token="sendingEmail" id="input_user_email">
        <label></label>
        <choice value="1">Cocher pour envoyer un email recapitulatif</choice>
      </input>
      <input type="text" token="email" depends="$sendingEmail$">
        <label>Adresse Email du destinataire</label>
        <default>$env:user_email$</default>
        <initialValue>$env:user_email$</initialValue>
      </input>
