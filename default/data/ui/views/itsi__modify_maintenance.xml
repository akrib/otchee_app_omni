    <panel>
      <input type="dropdown" token="pattern_type">
        <label>Pattern type</label>
        <choice value="exist">Pattern existant</choice>
        <choice value="new">Nouveau pattern</choice>
        <default>exist</default>
        <initialValue>exist</initialValue>
        <change>
          <condition match="$pattern_type$=&quot;exist&quot;">
            <set token="pattern_exist">1</set>
            <unset token="pattern_new"></unset>
            <unset token="dt_pattern_text"></unset>
          </condition>
          <condition match="$pattern_type$=&quot;new&quot;">
            <set token="pattern_new">1</set>
            <unset token="pattern_exist"></unset>
            <unset token="dt_patern_select"></unset>
          </condition>
        </change>
      </input>
      <input type="dropdown" token="dt_pattern_select" depends="$pattern_exist$">
        <label>Downtime Pattern</label>
        <change>
          <set token="dt_pattern_selected">$dt_pattern_select$</set>
        </change>
        <fieldForLabel>dt_pattern</fieldForLabel>
        <fieldForValue>dt_pattern</fieldForValue>
        <search>
          <query>
| inputlookup omni_kv_def 
| stats count as _c by dt_pattern
| appendpipe
[| stats count 
 | where count=0
 | eval dt_pattern="-"
 | table dt_pattern]
          </query>
        </search>
      </input>
      <input type="text" token="dt_pattern_text" depends="$pattern_new$">
        <label>Downtime pattern</label>
        <default></default>
        <change>
          <set token="dt_pattern_selected">$dt_pattern_text$</set>
        </change>
      </input>
    </panel>
